{
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "moduleResolution": "bundler",
      "strict": true,
      "jsx": "preserve",
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "lib": ["esnext", "dom"],
      "types": ["vue"]
    },
    "include": [
      "docs/**/*.ts",
      "docs/**/*.d.ts",
      "docs/**/*.tsx",
      "docs/**/*.vue"
    ]
  }